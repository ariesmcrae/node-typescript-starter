{
  "name": "typescript-starter",
  "version": "0.0.1",
  "description": "typescript node.js starter",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npx tsc",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "start": "npm i && npm run build && node dist/index.js",
    "start:local": "ts-node-dev --respawn src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ariesmcrae/node-typescript-starter.git"
  },
  "keywords": [
    "typescript",
    "node.js"
  ],
  "author": "Aries McRae <github@ariesmcrae.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ariesmcrae/node-typescript-starter/issues"
  },
  "homepage": "https://github.com/ariesmcrae/node-typescript-starter#readme",
  "devDependencies": {
    "@types/jest": "29.4.0",
    "@types/node": "18.13.0",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "eslint": "8.34.0",
    "eslint-config-standard-with-typescript": "34.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.4.0",
    "rimraf": "4.1.2",
    "ts-jest": "29.0.5",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "dotenv": "16.0.3"
  }
}
